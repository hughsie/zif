# Update a single package, with the old version having a provides required
# by another installed package.

local
	bash;0:0.3.0-1;i386;meta
		Requires
			zsh-oldstyle
	zsh;1.0.0-1;i386;meta
		Provides
			zsh-oldstyle

remote
	zsh;1.3.1-2;i386;meta

transaction
	update
		zsh

result
	bash;0:0.3.0-1;i386;meta
	zsh;1.0.0-1;i386;meta

config
	archinfo=i386
	skip_broken=1
