# update two packages, one of which does not exist in the remote repo

config
	archinfo=i386

local
	colord;0.0.1-1;i386;meta
	gnome-software;0.0.1;i386;meta

remote
	colord;0.0.2-1;i386;meta

transaction
	update
		colord;0.0.1-1;i386;meta
		gnome-software;0.0.1;i386;meta

result
	colord;0.0.2-1;i386;meta
	gnome-software;0.0.1;i386;meta
